  {
    "name": "curl",
    "releases_url": "",
    "tags_url": "",
    "tag_name": "curl-7_81_0",
    "download_url": "https://github.com/curl/curl/releases/download/curl-7_81_0/curl-7.81.0.tar.gz",
    "dependency": [
      "zlib-ng",
      "brotli",
      "zstd",
      "BoringSSL",
      "nghttp2",
      "libidn2",
      "libpsl"
    ],
    "cmd": [
      "mkdir tmp && cd tmp",
      "mkdir backup && cd backup",
      "cp /usr/local/lib/libidn2.a .",
      "cp /usr/local/lib/libpsl.a .",
      "cd ..",
      "mkdir libidn2 && cd libidn2",
      "gcc-ar -x /usr/local/lib/libidn2.a",
      "cd ..",
      "mkdir libunistring && cd libunistring",
      "gcc-ar -x /usr/local/lib/libunistring.a",
      "cd ..",
      "mkdir libpsl && cd libpsl",
      "gcc-ar -x /usr/local/lib/libpsl.a",
      "cd ..",
      "gcc-ar -qcs libidn2.a libidn2/*.o libunistring/*.o",
      "gcc-ranlib libidn2.a",
      "gcc-ar -qcs libpsl.a libidn2/*.o libunistring/*.o libpsl/*.o",
      "gcc-ranlib libpsl.a",
      "sudo cp libidn2.a /usr/local/lib/libidn2.a",
      "sudo cp libpsl.a /usr/local/lib/libpsl.a",
      "cd ..",
      "./configure --disable-debug --enable-optimize --disable-curldebug --enable-symbol-hiding --disable-ares --disable-ech --disable-code-coverage --disable-shared --enable-static --enable-http --disable-ftp --disable-file --disable-ldap --disable-ldaps --disable-rtsp --disable-dict --disable-telnet --disable-tftp --disable-pop3 --disable-imap --disable-smb --disable-smtp --disable-gopher --disable-mqtt --disable-manual --enable-libcurl-option --enable-libgcc --enable-ipv6 --enable-openssl-auto-load-config --enable-threaded-resolver --enable-verbose --disable-sspi --enable-crypto-auth --disable-ntlm --disable-ntlm-wb --disable-tls-srp --disable-unix-sockets --enable-cookies --disable-socketpair --enable-http-auth --enable-doh --enable-mime --enable-dateparse --disable-netrc --enable-progress-meter --enable-dnsshuffle --disable-get-easy-options --enable-alt-svc --enable-hsts --with-openssl --without-hyper --with-zlib --with-brotli --with-zstd --with-ca-bundle='/etc/ssl/certs/ca-certificates.crt' --with-ca-path='/etc/ssl/certs' --with-libpsl --without-libgsasl --without-libssh2 --without-libssh --without-librtmp --with-libidn2 --with-nghttp2 --without-ngtcp2 --without-nghttp3 --without-quiche",
      "make -j\"$(nproc)\"",
      "sudo make install",
      "cd tmp/backup",
      "sudo cp libidn2.a /usr/local/lib/libidn2.a",
      "sudo cp libpsl.a /usr/local/lib/libpsl.a",
      "cd ../..",
      "rm -rf tmp"
    ]
  },
