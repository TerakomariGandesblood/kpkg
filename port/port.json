{
  "export_gcc": "export CC=gcc-10 && export CXX=g++-10",
  "export_clang": "export CC=clang-12 && export CXX=clang++-12",
  "export_memory_flag": "export CFLAGS=\"-fsanitize=memory -fsanitize-memory-track-origins -fsanitize-memory-use-after-dtor -fno-omit-frame-pointer -fno-optimize-sibling-calls\" && export CXXFLAGS=\"-fsanitize=memory -fsanitize-memory-track-origins -fsanitize-memory-use-after-dtor -fno-omit-frame-pointer -fno-optimize-sibling-calls -stdlib=libc++\"",
  "export_thread_flag": "export CFLAGS=\"-fsanitize=thread\" && export CXXFLAGS=\"-fsanitize=thread -stdlib=libc++\"",
  "port": [
    {
      "name": "CMake",
      "releases_url": "",
      "tags_url": "https://api.github.com/repos/Kitware/CMake/tags",
      "tag_name": "",
      "download_url": "",
      "cwd": "",
      "cmd": [
        "./bootstrap --parallel=\"$(nproc)\" -- -DCMAKE_BUILD_TYPE:STRING=Release",
        "make -j\"$(nproc)\"",
        "sudo make install"
      ]
    },
    {
      "name": "ninja",
      "releases_url": "https://api.github.com/repos/ninja-build/ninja/releases/latest",
      "tags_url": "",
      "tag_name": "",
      "download_url": "",
      "cwd": "",
      "cmd": [
        "cmake -S . -B build -DCMAKE_BUILD_TYPE=Release",
        "cmake --build build --config Release -j\"$(nproc)\"",
        "sudo cmake --build build --config Release --target install"
      ]
    },
    {
      "name": "doxygen",
      "releases_url": "",
      "tags_url": "https://api.github.com/repos/doxygen/doxygen/tags",
      "tag_name": "",
      "download_url": "",
      "cwd": "",
      "cmd": [
        "cmake -S . -B build -DCMAKE_BUILD_TYPE=Release",
        "cmake --build build --config Release -j\"$(nproc)\"",
        "sudo cmake --build build --config Release --target install"
      ]
    },
    {
      "name": "lcov",
      "releases_url": "https://api.github.com/repos/linux-test-project/lcov/releases/latest",
      "tags_url": "",
      "tag_name": "",
      "download_url": "",
      "cwd": "",
      "cmd": ["sudo make install"]
    },
    {
      "name": "libc++",
      "releases_url": "",
